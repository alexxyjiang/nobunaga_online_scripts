Function GetWindowsWidth
	GetWindowsWidth = MainForm.InputBoxWidth.Text
End Function

Function GetWindowsHeight
	GetWindowsHeight = MainForm.InputBoxHeight.Text
End Function

Dimenv g_Hwnd, g_num
Dimenv g_tid1, g_tid2, g_tid3, g_tid4, g_tid5, g_tid6, g_tid7
Dimenv g_InvBase, g_MPBase, g_Base

Function GetInvUsedSize (hwnd)
	
	InvBase = g_InvBase
	size = Plugin.Memory.Read8Bit(hwnd, InvBase)
	GetInvUsedSize = size
	
End Function

Function GetMP(hwnd)
	
	MPBase = g_MPBase
	mp = Plugin.Memory.Read16Bit(hwnd, MPBase)
	
	GetMP = mp
	
End Function

Function UpdateInv(hwnd)
	
	Base = g_Base
	ID_Offset = &H04
	Num_Offset = &H0C
	Offset = &HD0
	
	size = GetInvUsedSize(hwnd)
	i = 0
	MainForm.ListBoxInv.list = ""
	For size
		
		current = Base + Offset * i
		num = Plugin.Memory.Read8Bit(hwnd, current + Num_Offset)
		item = Plugin.Memory.Read16Bit(hwnd, current + ID_Offset)
		text = item + " [" + num + "]"
		MainForm.ListBoxInv.AddItem (text)
		
		i = i + 1
		
	Next
	
End Function

Function GetSlotItem(hwnd, index)
	
	Base = g_Base
	ID_Offset = &H04
	Offset = &HD0
	Num_Offset = &H0C
	
	current = Base + Offset * index
	num = Plugin.Memory.Read8Bit(hwnd, current + Num_Offset)
	
	Dim item
	item = 0
	
	If num > 1 Then 
		item = Plugin.Memory.Read16Bit(hwnd, current + ID_Offset)
	End If
	
	GetSlotItem = item
	
End Function

Function isTrash(id)
	
	size = MainForm.ListBoxTrash.ListCount
	items = Split(MainForm.ListBoxTrash.list, "|")
	
	Dim value
	value = 0
	i = 0
	
	currentItem = cint(id)
	
	For size
		
		iter = cint(items(i))
		If iter = currentItem Then
			value = 1
			Exit For
		End If
		
		i = i + 1		
	Next
	
	isTrash = value
			
End Function

