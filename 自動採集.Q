 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot

	BackgroundSeqReverse = 0
End Function

Function ColtoInv(hwnd, num)

	TimeLong = cint(MainForm.InputBoxLongDelay.text)
	TimeShot = cint(MainForm.InputBoxShortDelay.text)
	TimeBias = cint(MainForm.InputBoxTinyDelay.text)

	Call Plugin.Bkgnd.RightDown(hwnd, 0, 0)
	Call Plugin.Bkgnd.RightUp(hwnd, 0, 0)
	Delay TimeShot
		
	// steps from collection to inventory	
	For num
 		Call Plugin.Bkgnd.KeyPress(hwnd, 73)
 		Delay TimeBias
	Next

 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeLong

	ColtoInv = 0
End Function

Function InvtoCol(hwnd, num)

	TimeLong = cint(MainForm.InputBoxLongDelay.text)
	TimeShot = cint(MainForm.InputBoxShortDelay.text)
	TimeBias = cint(MainForm.InputBoxTinyDelay.text)

	Call Plugin.Bkgnd.RightDown(hwnd, 0, 0)
	Call Plugin.Bkgnd.RightUp(hwnd, 0, 0)
	Delay TimeShot
		
	// steps from inventory	to collection
	For num
 		Call Plugin.Bkgnd.KeyPress(hwnd, 75)
 		Delay TimeBias
	Next

 	Call Plugin.Bkgnd.RightDown(hwnd, 0, 0)
	Call Plugin.Bkgnd.RightUp(hwnd, 0, 0)
	Delay TimeShot

	InvtoCol = 0
End Function

Function CleanInv(hwnd, start, num)

	TimeLong = cint(MainForm.InputBoxLongDelay.text)
	TimeShot = cint(MainForm.InputBoxShortDelay.text)
	TimeBias = cint(MainForm.InputBoxTinyDelay.text)

	Call Plugin.Bkgnd.RightDown(hwnd, 0, 0)
	Call Plugin.Bkgnd.RightUp(hwnd, 0, 0)
	Delay TimeShot
	
	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeLong
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	For num
 	// Cursor to the end
 		For 5
 			Call Plugin.Bkgnd.KeyPress(hwnd, 76)
 			Delay TimeBias
		Next
		
		// steps to the start
		For start
 			Call Plugin.Bkgnd.KeyPress(hwnd, 73)
 			Delay TimeBias
		Next
		
		// remove form the inv.
		
		Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 		Delay TimeShot
 		Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 		Delay TimeShot
 		
 		Call Plugin.Bkgnd.KeyPress(hwnd, 74)
 		Delay TimeBias
 		
 		Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 		Delay TimeLong
 		
 		Call Plugin.Bkgnd.KeyPress(hwnd, 73)
 		Delay TimeBias
	Next

 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot

	CleanInv = 0
End Function

Function BGCore(hwnd, steps, times, start, num, offset)
	
	Do		
		For times
			res = BackgroundSeq(hwnd, steps)
		Next
		
		Call ColtoInv(hwnd, offset)
		Call CleanInv(hwnd, start, num)
		Call InvtoCol(hwnd, offset)
				
	Loop
	
	BGCore = 0
	
End Function

Function BGCoreReverse(hwnd, steps, times, start, num, offset)
	
	Do		
		For times
			res = BackgroundSeqReverse(hwnd, steps)
		Next
		
		Call ColtoInv(hwnd, offset)
		Call CleanInv(hwnd, start, num)
		Call InvtoCol(hwnd, offset)
				
	Loop
	
	BGCoreReverse = 0
	
End Function

