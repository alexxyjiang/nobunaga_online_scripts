	InvtoCol = 0
End Function

Function CleanInv(hwnd, count)

	TimeLong = cint(MainForm.InputBoxLongDelay.text)
	TimeShot = cint(MainForm.InputBoxShortDelay.text)
	TimeBias = cint(MainForm.InputBoxTinyDelay.text)
	
	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	
	Call Plugin.Bkgnd.KeyPress(hwnd, 118)
	Delay TimeLong 

	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeLong
 	
 	
 	steps = 0
 	
 	For count
 		// Cursor to the end
 		For 5
 			Call Plugin.Bkgnd.KeyPress(hwnd, 76)
 			Delay TimeBias
		Next
		
		// steps to the delete point
		For steps
 			Call Plugin.Bkgnd.KeyPress(hwnd, 73)
 			Delay TimeBias
		Next
		
		last = GetInvUsedSize(hwnd) - 1
		offset = last - steps
		currentID = GetSlotItem(hwnd, offset)

		If isTrash(currentID) Then 
			//MainForm.LabDebug.Caption = "Blow away Item:" + currentID
			// remove form the inv.
			Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 			Delay TimeShot
 			Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 			Delay TimeShot
 			
 			Call Plugin.Bkgnd.KeyPress(hwnd, 74)
 			Delay TimeBias
 			
 			Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 			Delay TimeLong
 			
 			Call Plugin.Bkgnd.KeyPress(hwnd, 73)
 			Delay TimeBias
 		Else 
 			steps = steps + 1
 		End If
	Next

 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot

	CleanInv = 0
End Function

Function BGCore(hwnd, max, presve, key)
	
	Do
		Dim size

		Do
			Call BackgroundSeq(hwnd, key)
			Call BackgroundSeq(hwnd, key)
			Call BackgroundSeq(hwnd, key)
			
			do
				currentMp = cint(GetMP(hwnd))
				thresMp = cint(MainForm.InputBoxMP.text)
				If currentMp < thresMp Then
					Delay 10000
				Else 
					Exit do
				End If
			loop
			
			size = cint(GetInvUsedSize(hwnd))
			
			If size > max Then 
				Exit Do
			End If
		Loop
		
		num = size - presve
		
		Call ColtoInv(hwnd)
		
		If num > 0 then
			Call CleanInv(hwnd, num)
		End If
		
		Call InvtoCol(hwnd)
	Loop
	
	BGCore = 0
	
End Function

