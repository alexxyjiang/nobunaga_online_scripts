Function BackgroundSeq(hwnd, key)

	TimeLong = cint(MainForm.InputBoxLongDelay.text)
	TimeShot = cint(MainForm.InputBoxShortDelay.text)
	TimeBias = cint(MainForm.InputBoxTinyDelay.text)

  	Call Plugin.Bkgnd.KeyPress(hwnd, key)
	Delay TimeLong
		
  	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeLong
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeLong
	
  	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
 	Delay TimeShot
 	
 	Call Plugin.Bkgnd.KeyPress(hwnd, 27)
 	Delay TimeShot

	BackgroundSeq = 0
End Function

Function ColtoInv(hwnd)

	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
