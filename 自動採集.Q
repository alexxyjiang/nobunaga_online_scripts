Event MainForm.ButtonGetWin.Click

MsgBox "信L之野望 Online 8"
HwndEx = Plugin.Window.Search("信L之野望 Online 8")
temp = Split(HwndEx, "|")

g_Hwnd = ""

int i = 0

width = cint(GetWindowsWidth())
height = cint(GetWindowsHeight())

For UBound(temp)
	
	g_Hwnd = g_Hwnd + temp(i) + "|"
	Call Plugin.Window.Move(temp(i), i * width, 0)
	
	i = i + 1

